{
  "name": "zarm-web",
  "version": "0.0.1",
  "description": "基于 React 的桌面端UI库",
  "tags": [
    "zarm-web",
    "react",
    "zhongan"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "dragon-ui",
    "zarm-web",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "web"
  ],
  "license": "MIT",
  "repository": "git://github.com/JeromeLin/zarm-web",
  "bugs": {
    "url": "https://github.com/JeromeLin/zarm-web"
  },
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf types lib es dist assets coverage bundle components/*.js components/**/*.js components/**/**/*.js",
    "dev": "webpack-dev-server --config scripts/webpack/config.dev.js",
    "build:types": "rimraf types && tsc --outDir types -d --emitDeclarationOnly",
    "build:es": "rimraf es && babel components -d es -m es6 --extensions \".ts,.tsx\" --copy-files --config-file ./scripts/babel/config.es.js",
    "build:lib": "rimraf lib && babel components -d lib -m commonjs --extensions \".ts,.tsx\" --copy-files --config-file ./scripts/babel/config.lib.js",
    "build:umd": "rimraf dist && webpack --config scripts/webpack/config.umd.js && cross-env NODE_ENV=production webpack --config scripts/webpack/config.umd.js",
    "build:css": "rimraf lib/**/style/css.js lib/***/style/*.css es/**/style/css.js es/**/style/*.css && cd scripts/gulp && gulp",
    "build": "npm run build:lib && npm run build:es && npm run build:umd && npm run build:css",
    "deploy:build": "rimraf assets && webpack --config scripts/webpack/config.deploy.js",
    "deploy": "npm run deploy:build && gh-pages -d assets",
    "lint:ts": "eslint --ext .ts,.tsx components",
    "lint:js": "eslint --ext '.js,.jsx' site",
    "lint:style": "stylelint --syntax scss components/**/*.scss components/**/**/*.scss site/styles/**/*.scss",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:style",
    "test": "rimraf coverage && cross-env TZ=America/New_York jest --config scripts/jest/config.js --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "authors": "git log --format='%aN <%aE>' | sort -u | grep -v 'users.noreply.github.com' > AUTHORS",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -w -r 0",
    "release": "bash scripts/release.sh"
  },
  "dependencies": {
    "async-validator": "^1.10.1",
    "classnames": "2.2.6",
    "create-react-context": "0.2.3",
    "css-animation": "^1.2.5",
    "hoist-non-react-statics": "^3.3.0",
    "normalize.css": "8.0.1",
    "popper.js": "^1.15.0",
    "rc-animate": "^3.0.0-rc.5",
    "react-highlight": "^0.12.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.2",
    "@babel/runtime-corejs2": "^7.4.2",
    "@babel/standalone": "^7.0.0",
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/classnames": "^2.2.3",
    "@types/enzyme-to-json": "^1.5.0",
    "@types/jest": "^23.1.4",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "autoprefixer": "^9.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-react-hmre": "^1.1.1",
    "browserslist": "^4.4.1",
    "color": "^3.1.0",
    "conventional-changelog-cli": "^1.3.22",
    "core-js": "^3.0.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.1",
    "css-vars-ponyfill": "^2.0.2",
    "docsearch.js": "^2.6.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-za": "2.0.0-alpha.9",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^1.0.3",
    "gh-pages": "^1.0.0",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^23.3.0",
    "lint-staged": "^4.3.0",
    "marked": "^0.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "qrious": "^4.0.2",
    "raw-loader": "^0.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.0.0",
    "react-transition-group": "^2.2.1",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "stylelint-config-za": "^0.1.0",
    "through2": "^3.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "lint-staged": {
    "components/**/*.{ts,tsx}": "eslint",
    "{components,site}/**/*.{js,jsx}": "eslint",
    "{styles,site}/**/*.scss": "stylelint"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 9",
    "iOS >= 7",
    "Android >= 4"
  ],
  "sideEffects": [
    "dist/*",
    "components/style/*",
    "components/**/style/*",
    "es/**/style/*",
    "lib/**/style/*",
    "site/**"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
